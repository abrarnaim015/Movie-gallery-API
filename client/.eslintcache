       </div>\n              <button onClick={() => history.push('/home')} style={{ borderRadius: '20px' }} type=\"reset\" className=\"btn btn-outline-dark form-control\">More Movies</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </>\n  )\n}","/home/naim/Documents/PT Code Development Indonesia/Movie-gallery-API/client/src/pages/Filter-tgl-page.jsx",["77","78","79","80"],"/home/naim/Documents/PT Code Development Indonesia/Movie-gallery-API/client/src/style/animations/Home-page.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":36,"column":11,"nodeType":"87","endLine":36,"endColumn":123},{"ruleId":"88","severity":1,"message":"89","line":36,"column":54,"nodeType":"90","messageId":"91","endLine":36,"endColumn":74},{"ruleId":"92","severity":1,"message":"93","line":3,"column":16,"nodeType":"94","messageId":"95","endLine":3,"endColumn":21},{"ruleId":"92","severity":1,"message":"96","line":2,"column":25,"nodeType":"94","messageId":"95","endLine":2,"endColumn":42},{"ruleId":"85","severity":1,"message":"86","line":39,"column":11,"nodeType":"87","endLine":39,"endColumn":123},{"ruleId":"88","severity":1,"message":"89","line":39,"column":54,"nodeType":"90","messageId":"91","endLine":39,"endColumn":74},{"ruleId":"97","severity":1,"message":"98","line":67,"column":13,"nodeType":"87","endLine":67,"endColumn":85},{"ruleId":"85","severity":1,"message":"86","line":48,"column":13,"nodeType":"87","endLine":48,"endColumn":125},{"ruleId":"88","severity":1,"message":"89","line":48,"column":56,"nodeType":"90","messageId":"91","endLine":48,"endColumn":76},{"ruleId":"85","severity":1,"message":"86","line":73,"column":13,"nodeType":"87","endLine":73,"endColumn":125},{"ruleId":"88","severity":1,"message":"89","line":73,"column":56,"nodeType":"90","messageId":"91","endLine":73,"endColumn":76},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"jsx-a11y/anchor-is-valid","The href attr